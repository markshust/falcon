{
  "name": "@deity/falcon-ui",
  "license": "OSL-3.0",
  "version": "1.0.0",
  "repository": "https://github.com/deity-io/falcon/tree/master/packages/falcon-ui",
  "main": "dist/falcon-ui.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:tsDeclarations && npm run size",
    "build:tsDeclarations": "tsc",
    "build:esm": "babel src -d dist -x .ts,.tsx -s",
    "build:cjs": "rollup -c",
    "size": "size-limit",
    "start": "docz dev",
    "watch": "babel src -d dist -x .ts,.tsx -s --watch",
    "docs": "rimraf .docz && docz build"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@deity/falcon-theme-editor": "1.0.0",
    "@mdx-js/tag": "^0.15.0",
    "@types/deepmerge": "^2.1.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "babel-plugin-annotate-pure-calls": "^0.3.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "docz": "^0.11.2",
    "docz-plugin-netlify": "^1.1.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "size-limit": "^0.20.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@emotion/core": "^0.13.1",
    "@emotion/is-prop-valid": "^0.6.8",
    "@emotion/provider": "^0.11.2",
    "@emotion/styled-base": "^0.10.6",
    "csstype": "^2.5.7",
    "deepmerge": "^2.1.1",
    "is-plain-object": "^2.0.4",
    "react-powerplug": "^1.0.0-rc.1"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "gzip": false,
      "limit": "60 KB"
    }
  ]
}
